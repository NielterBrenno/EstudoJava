
public class Livro {
	
	String nome;
	String descricao;
	double Valor;
	String isbn;
	String nomeDoAutor;
	
	
	void mostrarDetralhes() {
		
		String mensagem = "***********Mostrando Detralhes do Livro*********** ";
		System.out.println(mensagem);
		System.out.println(nome);
		System.out.println(descricao);
		System.out.println(Valor);
		System.out.println(isbn);
		System.out.println("--");
		
	}
	

}
